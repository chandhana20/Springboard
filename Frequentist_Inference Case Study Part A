      "source": [
        "__A:__"
      ]
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "114N5Rkx1yX3"
      },
      "source": [
        "def multi_sampler(d, n):\n",
        "    means = []\n",
        "    for i in range(d):\n",
        "        daily_sample = townsfolk_sampler(n)\n",
        "        xbar = np.mean(daily_sample)\n",
        "        means.append(xbar)\n",
        "    return means"
      ],
      "execution_count": 20,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "PkWsCpW31yX3"
